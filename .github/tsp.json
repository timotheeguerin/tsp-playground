{
  "problemMatcher": [
    {
      "owner": "tsp",
      "pattern": [
        {
          "regexp": "^(?:\\x1b\\[[\\d;]+m)*([^\\s].*)[\\(:](?:\\x1b\\[[\\d;]+m)*(\\d+)[,:](?:\\x1b\\[[\\d;]+m)*(\\d+)(?:\\):\\s+|\\s+-\\s+)(?:\\x1b\\[[\\d;]+m)*(error|warning|info)\\s+([a-z0-9\\-]+)\\s*:\\s*(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
